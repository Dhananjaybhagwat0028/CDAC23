class  Pattern_11{
	public static void main(String[]args)
	{
		for(char i='a';i<='e';i++)
		{
			
			for(char j='e';j>=i;j--)
			{
				System.out.print(" ");
			}
			
			for(char k='a';k<=i;k++)
		  {
			System.out.print("* ");
		  }
		  
		 System.out.println();
	    }
		
}
}

